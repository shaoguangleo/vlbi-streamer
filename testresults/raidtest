for i in sdc sdd sde sdf sdg sdh sdl sdn sdo sdq sdr sds sdt ; do parted /dev/$i rm 1 ; parted /dev/$i mkpart p 0GB 500GB ;done

for i in $DRIVES ; do mkfs.ext4 $i ;  tune2fs -o journal_data_writeback $i; tune2fs -O ^has_journal $i ; done

DRIVES="/dev/sdb1 /dev/sdc1 /dev/sdd1 /dev/sde1 /dev/sdg1 /dev/sdh1 /dev/sdi1 /dev/sdj1 /dev/sdk1 /dev/sdl1 /dev/sdm1 /dev/sdn1 /dev/sdo1 /dev/sdp1 /dev/sdq1 /dev/sdr1 /dev/sds1 /dev/sdt1"

mdadm --create --verbose /dev/md0 --level=0 --raid-devices=18 $DRIVES
mkfs.ext4 /dev/md0; tune2fs -o journal_data_writeback /dev/md0; tune2fs -O ^has_journal /dev/md0
MOUNT_OPTS="defaults,data=writeback,noatime,nodiratime"; mount /dev/md0 -o ${MOUNT_OPTS} /mnt/disk0

